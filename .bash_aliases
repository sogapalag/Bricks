# pbcopy OSX
alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'

alias jn='jupyter notebook'

# proxy, well 18.04 via setting also work
alias fkwall='export all_proxy=socks5://127.0.0.1:1080'

# g++
alias ff='g++ -std=c++14 -O2 -Wall -pthread'

# open snip
alias snip='vim ~/.vim_runtime/sources_non_forked/vim-snippets/snippets/cpp.snippets'
